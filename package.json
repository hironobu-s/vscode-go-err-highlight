{
    "name": "go-err-highlight",
    "displayName": "Go err highlight",
    "description": "Highlight an err variable in Go source code.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "path": "./syntaxes/injection.json",
                "scopeName": "source.go-err",
                "injectTo": [
                    "source.go"
                ]
            }
        ]
    }
}