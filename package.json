{
    "name": "go-err-highlighting",
    "displayName": "Go err highlighting",
    "description": "Highlight an err variable in Go source code.",
    "version": "1.0.2",
    "publisher": "hiro",
    "repository": {
        "url": "https://github.com/hironobu-s/vscode-go-err-highlight"
    },
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "path": "./syntaxes/injection.json",
                "scopeName": "source.go-err",
                "injectTo": [
                    "source.go"
                ]
            }
        ]
    }
}